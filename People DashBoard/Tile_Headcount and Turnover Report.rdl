  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT A.MonthDate, year(A.monthdate) as Year,Month(A.Monthdate) as MonthNo,Datename(Month,A.monthdate) as Month,
   -- SUM(C.EmpCount) AS Employees,   
    SUM(A.EmpCount) AS EmployeesMonth,   
    --SUM(A.TurnOverCount) AS Turnover,
	 SUM(A.TurnOverCountMonth) AS TurnoverMonth 
 FROM AL_TurnoverYearMonthVW A (NOLOCK)
 inner join SN_SITES B  (NOLOCK) on A.siteid = B.siteid
 Inner join SN_ST4_Users C (NOLOCK) on A.siteid = C.siteid
    WHERE A.SITEID = B.SITEID AND B.Active = 1  
    AND A.Year BETWEEN YEAR(GETDATE()) -2 AND YEAR(GETDATE())  
    --AND B.SITE_ID IN (' + @SITELIST + ')       
    --AND A.YEAR IN (' + @YEARLIST + ')       
    AND C.username = 'CRPACCAR'and C.active = 1
 	and A.monthdate &gt; DATEADD(m, - 20, GETDATE())
    GROUP BY A.YEAR, A.MONTH, A.MonthDate
	order by year,monthno
	</CommandText>
      </Query>
      <Fields>
        <Field Name="MonthDate">
          <DataField>MonthDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MonthNo">
          <DataField>MonthNo</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="EmployeesMonth">
          <DataField>EmployeesMonth</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TurnoverMonth">
          <DataField>TurnoverMonth</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
