  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT Year, Month,Datename(Month,Monthdate) as MonthName,
(cast(Datename(Month,Monthdate) as varchar(3) )+ ' ' + CAST(YEAR(MonthDate) as Varchar(4))) as MonthYear 
, Quarter, MonthDate, AwardTypeId, AwardType,RecogTypeId, RecogType,
SUM(EmpRecogCount) As EmpRecogCount, SUM(AwardTotal) As AwardTotal   
FROM AN_ERSummary (NOLOCK)  
GROUP BY Year, Month, Quarter, MonthDate, AwardTypeId, AwardType,RecogTypeId, RecogType
</CommandText>
      </Query>
