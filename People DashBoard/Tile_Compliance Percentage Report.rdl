  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT (A.Site), A.Position, A.MonthDate,Year(A.Monthdate) as Year,Month(A.MonthDate) as Month,Datename(Month,A.Monthdate) as MonthName,
 SUM(A.Required) As Required, SUM(A.Actual) As Actual,   
CAST(CAST(ROUND( CASE WHEN SUM(A.Required) &gt; 0 THEN SUM(A.Actual) * 100/SUM(A.Required) ELSE 0 END,0) AS INTEGER) AS NVARCHAR)  + ' %' As Compliance
 FROM AN_TrainingStsVW A (NOLOCK) 
Inner join  SN_ST4_Users B on A.site_id = B.site_id
 WHERE
 -- Site_Id IN (' + @SITELIST + ') AND Year IN (' + @YEARLIST + ') AND MonthNo IN (' + @MONTHLIST + ')  AND 
 A.Monthdate &gt; DATEADD(m, -1, GETDATE()) and
  B.username = 'Southernregion'and B.active = 1 
  GROUP BY Site, Position, Year, Quarter, MonthNo, MonthDate 
  ORDER BY Site, Position,year,month</CommandText>
      </Query>
