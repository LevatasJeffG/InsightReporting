  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT A.Reason, SUM(A.ReasonCount) AS ReasonCount  
 FROM AL_TurnoverReasonVW A (NOLOCK)
 inner join SN_SITES B (NOLOCK)  on A.SiteId = B.SiteId AND B.Active = 1 
 Inner join SN_ST4_Users C (NOLOCK) on A.siteid = C.siteid and C.Active =1
 AND A.Month IN ('10') AND A.Year IN ('2016') and C.username = 'CRPACCAR'
 --AND B.Site_Id IN (' + @SITELIST + ')   
 GROUP BY  A.Reason ORDER BY SUM(A.ReasonCount) DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="Reason">
          <DataField>Reason</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ReasonCount">
          <DataField>ReasonCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
