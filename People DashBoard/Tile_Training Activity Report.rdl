  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select A.Position,
--A.Year,A.MonthNo,Datename(Month,A.monthDate) as Monthname,
Sum(B.courseHours) as CourseHours from AN_TrainingSummary A
inner join AN_TrainingDetail B on A.site_id = B.site_id
inner join  SN_SITES C on A.site_id = C.Site_id and C.Active = 1
Inner join  SN_ST4_Users D on B.siteid = D.siteid and D.Active =1
where D.username = 'Southernregion' and
--and A.monthno = '9'
A.MonthDate &gt;DATEADD(m, - 14, GETDATE())
group by  A.Position</CommandText>
      </Query>
      <Fields>
        <Field Name="Position">
          <DataField>Position</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CourseHours">
          <DataField>CourseHours</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
