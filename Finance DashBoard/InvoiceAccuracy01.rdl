  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
A.Company_Name, A.SBU_Name, A.Job_Name as Site, 
A.State_Name, A.City,
 A.Year_No, A.Month_No, A.Month_Name, A.Quarter_Name,A.TWC_INVOICE_CNT as Invoices, 
A.TWC_CREDIT_CNT as Adjustments
--,sum( A.TWC_INVOICE_CNT) as Invoices, sum(A.TWC_CREDIT_CNT) as Adjustments
FROM DM_PS_Invoice_Accuracy A 
INNER JOIN SN_Sites B ON A.Job_ID = B.SiteId
INNER JOIN dbo.SN_ST4_Users C ON A.Job_ID = C.SiteId 
  
WHERE C.UserName = 'Southernregion' AND C.ACTIVE = 1 and  B.Active = 1 And A.Month_no = '2' and
(CAST(CONVERT(VarChar(2), A.Month_No) + '/1/' + CONVERT(VarChar(4), A.Year_No) AS Date)  &gt; DATEADD(m, - 20, GETDATE())) 
order by JOb_Name</CommandText>
      </Query>
