      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>e9473910-5ada-4757-933f-34057fa0c047</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@monthno">
            <Value>=Parameters!monthno.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
  Select A.site_id,A.Name,A.Count as 'No of Inspections '
  from AN_Inspection_Month A 
  Inner join SN_ST4_Users B on A.site_id = B.Site_id and A.Active = 1
 where (CAST(CONVERT(VarChar(2), MonthNo) + '/1/' + CONVERT(VarChar(4), Year) AS Date)  &gt; DATEADD(m, - 16, GETDATE())) AND B.Active = 1
 and B.username = 'Southernregion' and 
   monthno = @monthno</CommandText>
      </Query>
      <Fields>
        <Field Name="site_id">
          <DataField>site_id</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="No_of_Inspections_">
          <DataField>No of Inspections </DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
